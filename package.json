{
  "name": "@shadowrz/hanekokoro-ui",
  "version": "0.1.0",
  "description": "Personal Design System by @ShadowRZ",
  "sideEffects": false,
  "private": true,
  "type": "module",
  "files": [
    "dist",
    "src/index.tsx",
    "src/components"
  ],
  "scripts": {
    "build": "rm -rf dist && pnpm build:vite && pnpm build:solid",
    "build:vite": "vite build",
    "build:solid": "node esbuild.config.mjs",
    "prepare": "panda codegen",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "keywords": [],
  "author": {
    "name": "Yorusaka Miyabi"
  },
  "exports": {
    ".": {
      "solid": "./dist/solid/index.jsx",
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./panda-preset": {
      "types": "./dist/types/panda-preset.d.ts",
      "import": "./dist/es/panda-preset.js",
      "require": "./dist/cjs/panda-preset.js"
    }
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@ark-ui/anatomy": "^3.5.0",
    "@ark-ui/solid": "^4.2.0",
    "@eslint/js": "^9.14.0",
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource-variable/outfit": "^5.1.0",
    "@iconify-json/ph": "^1.2.1",
    "@pandacss/dev": "^0.47.0",
    "@radix-ui/colors": "^3.0.0",
    "@storybook/addon-a11y": "^8.4.4",
    "@storybook/addon-docs": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-mdx-gfm": "^8.4.4",
    "@storybook/addon-themes": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/docs-tools": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@storybook/types": "^8.4.4",
    "@types/eslint__js": "^8.42.3",
    "chromatic": "^11.16.5",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.0",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "remark-gfm": "^4.0.0",
    "solid-js": "^1.9.3",
    "storybook": "^8.4.4",
    "storybook-solidjs": "^1.0.0-beta.6",
    "storybook-solidjs-vite": "^1.0.0-beta.6",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "unplugin-icons": "^0.20.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "@ark-ui/solid": "^4.0.0",
    "solid-js": "^1.8.19"
  },
  "pnpm": {
    "overrides": {
      "@storybook/builder-vite@next": "^8.4.2"
    }
  },
  "packageManager": "pnpm@9.12.3"
}
